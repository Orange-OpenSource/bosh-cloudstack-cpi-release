---

cloudstack:
  endpoint: <%= p('cloustack.endpoint') %>
  api_key: <%= p('cloustack.api_key') %>
  secret_access_key: <%= p('cloustack.secret_access_key') %>
  default_key_name: <%= p('cloustack.default_key_name') %>
  private_key: <%= p('cloustack.private_key') %>
  state_timeout: <%= p('cloustack.state_timeout') %>
  state_timeout_volume: <%= p('cloustack.state_timeout_volume') %>
  stemcell_public_visibility:<%= p('cloustack.stemcell_public_visibility') %>
  default_zone: <%= p('cloustack.default_zone') %>

logging:
  level: DEBUG


pidfile: /var/vcap/sys/run/cpi/cpi.pid

server:
  port: <%= p('cpi.port') %>
